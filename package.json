    "postcss-safe-parser": "4.0.1",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
    "semver": "6.3.0",
    "sinon": "^11.1.2",
    "style-loader": "0.23.1",
    "styled-components": "^5.1.1",
    "terser-webpack-plugin": "2.3.5",
    "ts-node": "^9.1.1",
    "ts-pnp": "1.1.6",
    "typescript": "^4.2.4",
    "url-loader": "2.3.0",
    "webpack": "4.42.0",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "scripts": {
    "react": "node scripts/start.js",
    "build-react": "node scripts/build.js",
    "reactTs:build": "tsc --project tsconfig.react.json",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test:frontend": "node scripts/test.js",
    "test:integration": "cross-env NODE_ENV=test ts-mocha --project ./tsconfig.express.json ./tests/integration/*.test.ts --exit",
    "test:unit": "cross-env NODE_ENV=test ts-mocha --project ./tsconfig.express.json ./tests/unit/*.test.ts --exit",
    "start:server": "cd ../recipe-book-server && python -m server",
    "dev": "nodemon --watch src -e ts --exec yarn dev:start",
    "dev:start": "npm-run-all express:build express:start",
    "express:build": "tsc --project tsconfig.express.json",
    "express:start": "cross-env NODE_ENV=development babel-node ./build/src/index.js",
    "build": "cross-env NODE_ENV=production npm-run-all express:build build-react",
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "docker:dev": "docker-compose run --service-ports dev-db ",
    "docker:test": "docker-compose run --service-ports test-db ",
    "lint": "eslint ./src/*",
    "migrate:make": "knex migrate:make",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed:make": "knex seed:make",
    "seed:run": "knex seed:run"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.21",
    "@types/chai-http": "^4.2.0",
    "@types/connect-pg-simple": "^4.2.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/keygrip": "^1.0.2",
    "@types/mocha": "^9.0.0",
    "@types/mongoose": "^5.10.3",
    "@types/request": "^2.48.7",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "mocha": "^9.1.0",
    "ts-mocha": "^8.0.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
